{% extends '::ojsbase.html.twig' %}
{% block title %}{{ 'title.section_new'|trans }} {{ parent() }}{% endblock %}

{% block breadcrumb %}
    {% set list = [
    {'link': path('ojs_journal_dashboard_index', {journalId: selectedJournal().id}), 'title': 'dashboard'|trans},
    {'link': path('ojs_journal_section_index', {'journalId': selectedJournal().id}),'title': 'title.sections'|trans},
    {'title': 'title.section_new'|trans}
    ] %}
    {{ breadcrumb(list) }}
{% endblock %}

{% block body -%}
    <h1>{{ 'title.section_new'|trans }}</h1>

    {{ form(form) }}

    <a href="{{ path('ojs_journal_section_index', {'journalId': selectedJournal().id}) }}">
        {{ "b"|trans }}
    </a>
{% endblock %}
